{
  "name": "team-manager-api",
  "version": "1.0.0",
  "description": "A team management API built with Node.js, Express, TypeScript, and PostgreSQL",
  "main": "dist/server.js",
  "scripts": {
    "dev:local": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "ts-node prisma/seed.ts",
    "test": "echo \"Tests not implemented yet\" && exit 0"
  },
  "keywords": ["api", "express", "typescript", "postgresql", "prisma"],
  "author": "Alex Denbaugh",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "prisma": "^6.1.0",
    "@types/node": "^22.10.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
